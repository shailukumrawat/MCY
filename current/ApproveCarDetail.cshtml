    function loadFuel() {
        var fuelid = '@Model.fuel';
        $.ajax({
            type: "POST",
            contentType: "application/json; charset=utf-8",
            //url is the path of our web method (Page name/function name)
            dataType: "json",
            url: '/Admin/GetAllFuel',
            asyn: false,
            cache: false,
            //called on jquery ajax call success
            success: function (result) {
                $('#fuel').empty();
                $("#fuel").append($("<option></option>").val("0").html("Select Fuel"));
                for (var i = 0; i <= result.fuellist.length - 1; i++) {
                    if (result.fuellist[i].status == 1) {
                        if (result.fuellist[i].fuel_id == fuelid) {

                            $("#fuel").append($("<option selected></option>").val(result.fuellist[i].fuel_id).html(result.fuellist[i].fuel_name));

                        } else {
                            $("#fuel").append($("<option></option>").val(result.fuellist[i].fuel_id).html(result.fuellist[i].fuel_name));
                        }
                    }
                }
            },
            //called on jquery ajax call failure
            error: function ajaxError(result) {
                $('#fuel').empty();
                $("#fuel").append($("<option></option>").val("0").html("Select Fuel"));
            }
        });

    }


