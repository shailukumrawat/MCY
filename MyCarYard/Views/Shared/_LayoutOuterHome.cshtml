                        <script>
                            function SentEnquiry() {

                                try {
                                    var isvalid = true;

                                    var enqname = $('#enqname').val();
                                    if (enqname.trim() == "" || enqname == undefined || enqname == null) {

                                        $('#enqnamevalid').html("Please enter name");
                                        isvalid = false;

                                    } else {

                                        $('#enqnamevalid').html("");

                                    }

                                    var enqemail = $('#enqemail').val();

                                    if (enqemail.trim() == undefined || enqemail == null || enqemail == "") {
                                        $('#enqemailvalid').html("Please Enter Email Address");
                                        isvalid = false;
                                    } else {
                                        $('#enqemailvalid').html("");
                                    }
                                    var regex = /^[a-z0-9\.\_%+-]+@@[a-z0-9\.\-]+\.[a-z]{2,4}$/i;

                                    if (enqemail.search(regex)) {

                                        $('#enqemailvalid').html("Please Enter Valid Email Id");
                                        isvalid = false;
                                    } else {

                                        $('#enqemailvalid').html("");
                                        // isvalid = true;

                                    }

                                    var enqcno = $('#enqcno').val();
                                    if (enqcno.trim() == "" || enqcno == null || enqcno == undefined) {

                                        $('#enqcnovalid').html("Please Enter Contact Number");
                                        isvalid = false;
                                    } else {


                                        $('#enqcnovalid').html("");

                                    }

                                    var enqpostcode = $('#enqpostcode').val();

                                    if (enqpostcode == null || enqpostcode == undefined || enqpostcode.trim() == "") {


                                        $('#enqpostcodevalid').html("Please Enter Post Code");
                                        isvalid = false;
                                    } else {

                                        $('#enqpostcodevalid').html("");
                                    }

                                    var comment = $('#enqcomment').val();
                                    var custemail = $('#custemail').val();
                                    if (isvalid == true) {

                                        var obj = { "enqname": enqname, "email": enqemail, "cno": enqcno, "postcode": enqpostcode, "msg": comment, "custemail": custemail };

                                        $.ajax({
                                            type: "POST",
                                            contentType: "application/json; charset=utf-8",
                                            //url is the path of our web method (Page name/function name)
                                            dataType: "json",
                                            data: JSON.stringify(obj),
                                            url: '/Admin/SendEnquiry',
                                            asyn: false,
                                            cache: false,
                                            //called on jquery ajax call success
                                            success: function (result) {
                                                if (result.status == "Success") {

                                                    $('#cmessage2').html("Your message has been sent");
                                                    $('#enqname').val('');
                                                    $('#enqcomment').val('');
                                                    $('#enqpostcode').val('');
                                                    $('#enqcno').val('');
                                                    $('#enqemail').val('');
                                                    isvalid = false;

                                                }
                                            },
                                            //called on jquery ajax call failure
                                            error: function ajaxError(result) {
                                                // alert("Invalid");
                                                isvalid = false;
                                            }
                                        });


                                    }
                                    return isvalid;
                                } catch (e) {

                                    console.log(e);
                                }
                            }
                        </script>

