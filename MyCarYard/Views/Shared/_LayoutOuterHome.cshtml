        <script>

            function validate() {

                var isvalid = true;
                try {
                    var email = $('#Email').val();
                    var password = $('#Password').val().trim();

                    if (email == undefined || email == null || email == "") {
                        $('#emailvalid').html("Please Enter Email Address");
                        isvalid = false;
                    } else {
                        $('#emailvalid').html("");
                    }
                    var regex = /^[a-z0-9\.\_%+-]+@@[a-z0-9\.\-]+\.[a-z]{2,4}$/i;

                    if (email.search(regex)) {

                        $('#emailvalid').html("Please Enter Valid Email Id");
                        isvalid = false;
                    } else {

                        $('#emailvalid').html("");
                        isvalid = true;

                    }

                    if (password == undefined || password == null || password == "") {
                        $('#passvalid').html("Please Enter Password");
                        isvalid = false;
                    } else {
                        $('#passvalid').html("");
                    }

                    if (isvalid == true) {
                        $.ajax({
                            type: "POST",
                            contentType: "application/json; charset=utf-8",
                            //url is the path of our web method (Page name/function name)
                            dataType: "json",
                            data: "{ unm: '" + email + "',pass: '" + password + "' }",
                            url: '/Admin/checklogin',
                            asyn: false,
                            cache: false,
                            //called on jquery ajax call success
                            success: function (result) {
                                if (result.status == "Success") {
                                    var Check = $("input[name='RememberMe']").is(":checked");

                                    if ($("input[name='RememberMe']").prop("checked")) {
                                        RememberMeCheck(1, email, password);
                                    } else { RememberMeCheck(1, "", ""); }
                                    $('#invalidunm').html("");
                                    $("#formLogin").submit();
                                } else {
                                    $('#invalidunm').html("Invalid UserName Or Password");
                                    isvalid = false;
                                }
                            },
                            //called on jquery ajax call failure
                            error: function ajaxError(result) {
                                //alert("Data Not Found");
                            }
                        });


                    }



                }
                catch (e) {
                    console.log(e);
                    isvalid = false;
                }


                return isvalid;
            }

            var RememberMeCheck = function (getStatus, userIdentity, userPassword) {
                if (getStatus == 1) {
                    localStorage.setItem("u_Id", userIdentity);
                    localStorage.setItem("u_Ps", userPassword);
                } else {
                    $('#Email').val(localStorage.getItem("u_Id"));
                    $('#Password').val(localStorage.getItem("u_Ps"));
                    if (localStorage.getItem("u_Id") != "") {
                        jQuery("input[name='RememberMe']").prop('checked', true);
                    }
                }

            };
            $(Document).ready(function () {
                RememberMeCheck(0, "", "");
            })
        </script>

