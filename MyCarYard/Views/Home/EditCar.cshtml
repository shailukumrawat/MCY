<script type="text/javascript">
    $(document).ready(function () {
        loadMake();
        loadSpeedtype();
    });

    function loadSpeedtype() {
        // alert("hi");
        var speedtype = '@Model.speedtype';
        $.ajax({
            type: "POST",
            contentType: "application/json; charset=utf-8",
            //url is the path of our web method (Page name/function name)
            dataType: "json",
            url: '/Admin/LoadSpeedType',
            asyn: false,
            cache: false,
            //called on jquery ajax call success
            success: function (result) {
                $('#speedtype').empty();
                $("#speedtype").append($("<option selected></option>").val("0").html("Select Speedtype"));

                for (var i = 0; i <= result.speedtypelist.length - 1; i++) {

                    if (result.speedtypelist[i].status == "1") {

                        if (result.speedtypelist[i].speedtypeid == speedtype) {

                            $("#speedtype").append($("<option selected></option>").val(result.speedtypelist[i].speedtypeid).html(result.speedtypelist[i].speedtypename));
                        } else {
                            $("#speedtype").append($("<option></option>").val(result.speedtypelist[i].speedtypeid).html(result.speedtypelist[i].speedtypename));
                        }
                    }
                }
            },
            //called on jquery ajax call failure
            error: function ajaxError(result) {
                $('#speedtype').empty();
                $("#speedtype").append($("<option selected></option>").val("0").html("Select Speedtype"));

            }
        });
    }



    function loadMake() {

        $body = $("body");
        $body.addClass("loading");

        var makeid = '@Model.make';
       // alert("hi");
        $.ajax({
            type: "POST",
            contentType: "application/json; charset=utf-8",
            //url is the path of our web method (Page name/function name)
            dataType: "json",
            url: '/Admin/LoadMake',
            asyn: false,
            cache: false,
            //called on jquery ajax call success
            success: function (result) {
                $('#make').empty();
                $("#make").append($("<option></option>").val("0").html("Select Make"));

                for (var i = 0; i <= result.makelist.length - 1; i++) {

                    if (result.makelist[i].status == "1") {

                        if (result.makelist[i].make_id == makeid) {

                            $("#make").append($("<option selected></option>").val(result.makelist[i].make_id).html(result.makelist[i].make));

                            loadModel();
                        } else {

                            $("#make").append($("<option></option>").val(result.makelist[i].make_id).html(result.makelist[i].make));
                        }
                    }
                }

                $body.removeClass("loading");
            },
            //called on jquery ajax call failure
            error: function ajaxError(result) {
                alert("Data Not Found");
            }
        });
    }
</script>
<script type="text/javascript">
    function loadModel() {

        $body = $("body");
        $body.addClass("loading");

        var make = $('#make option:selected').val();
        var modelid = '@Model.model';
       // alert(modelid);
        //  alert(count_id);
        $.ajax({
            type: "POST",
            contentType: "application/json; charset=utf-8",
            //url is the path of our web method (Page name/function name)
            dataType: "json",
            data: "{ make: '" + $("#make").val() + "' }",
            url: '/Admin/LoadModel',
            asyn: false,
            cache: false,
            //called on jquery ajax call success
            success: function (result) {

                $('#model').empty();
                $("#model").append($("<option></option>").val("0").html("Select Model"));

                for (var i = 0; i <= result.modellist.length - 1 ; i++) {

                    if (result.modellist[i].status == "1") {

                        if (result.modellist[i].model_id == modelid) {

                            //alert(result.modellist[i].model_id);
                            $("#model").append($("<option selected></option>").val(result.modellist[i].model_id).html(result.modellist[i].modeltype));

                            loadBadge();
                        } else {

                            $("#model").append($("<option></option>").val(result.modellist[i].model_id).html(result.modellist[i].modeltype));
                        }
                    }
                }

                $body.removeClass("loading");
            },
            //called on jquery ajax call failure
            error: function ajaxError(result) {
                // alert("Data Not Found");
            }
        });
