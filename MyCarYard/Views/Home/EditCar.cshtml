@model MyCarYard.Models.ApproveUserListModel
@{
    ViewBag.Title = "Edit Car Details";
    Layout = "~/Views/Shared/_LayoutOuterUser.cshtml";
}


<div class="col-sm-7">
    <div class="mngevnt_contain ">
        <div class="detail_head event_head">
            <h4 class="detail_heading">Edit Car Detail </h4>
        </div>
        <div class="describe_contain">
            <h4 class="seller_head">Car Images</h4>
            <div class="row">
                <div class="col-sm-12">
                    <form action="EditCarPic" method="post" enctype="multipart/form-data" id="uploadcarpic" name="frm1">
                        <div class="event_upload">
                            <ul>
                                <li id="input_file">
                                    <i class="flaticon1-arrows"></i>
                                    <input type="file" class="event_upldnew" id="img" name="img" onchange="loadFile(event)"> 
