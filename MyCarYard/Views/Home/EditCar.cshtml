    <input type="text" placeholder="Engine Size" class="form-control" id="engine" value="@Model.engine">
                        *@
                    </div>
                </div>

                <div class="col-sm-6">
                    <div class="form-group">
                        <label>Fuel</label>
                        <select class="form-control slt_cstm" id="fuel">
                         

                        </select>
                    </div>
                </div>
                <div class="col-sm-6">
                    
                    <div class="form-group hidden">
                        <label>Status</label>
                        <select class="form-control slt_cstm" id="gstatus">
                            <option value="">Select Status</option>
                            @if (Model.gstatus == "0")
                            {
                                <option >Garaged</option>
                                <option selected>For Sale</option>
                            }
                            else
                            {

                                <option selected>Garagged</option>
                                <option >For Sale</option>
                            }

                            
                        </select>
                    </div>
                </div>

                <div class="col-sm-12">
                    <div class="form-group">
                        <label>Car Description</label>
                        <textarea placeholder="Car Description" class="form-control" id="descr"  onkeyup='charLimit(this, 1000);'>@Model.descr</textarea>
                    </div>
                </div>
                <script>
                        function charLimit(limitField, limitNum) {
                            if (limitField.value.length > limitNum) {
                                limitField.value = limitField.value.substring(0, limitNum);}
                        }


                </script>
                <div class="col-sm-12">

