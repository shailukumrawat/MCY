                                    <img id="output" class="upload_img" src="~/images/cars/@Model.img" />
                                </li>
                                <li>
                                    <i class="flaticon1-arrows"> </i>
                                    <input type="file" class="event_upldnew" id="img1" name="img1" onchange="loadFile1(event)">
                                    Upload Car Image Here 2
                                    <img id="output1" class="upload_img" src="~/images/cars/@Model.img1" />
                                </li>
                                <li>
                                    <i class="flaticon1-arrows"> </i>
                                    <input type="file" class="event_upldnew" id="img2" name="img2" onchange="loadFile2(event)">
                                    Upload Car Image Here 3
                                    <img id="output2" class="upload_img"  src="~/images/cars/@Model.img2"/>
                                </li>
                            </ul>
                            <ul>
                                <li id="input_file">
                                    <i class="flaticon1-arrows"> </i>
                                    <input type="file" class="event_upldnew" id="img3" name="img3" onchange="loadFile3(event)"> 
                                    Upload Car Image Here 4
                                    <img id="output3" class="upload_img" src="~/images/cars/@Model.img3" />
                                </li>
                                <li>
                                    <i class="flaticon1-arrows"> </i>
                                    <input type="file" class="event_upldnew" id="img4" name="img4" onchange="loadFile4(event)">
                                    Upload Car Image Here 5
                                    <img id="output4" class="upload_img" src="~/images/cars/@Model.img4" />
                                </li>
                                <li>
                                    <i class="flaticon1-arrows"> </i>
                                    <input type="file" class="event_upldnew" id="img5" name="img5" onchange="loadFile5(event)">
                                    Upload Car Image Here 6
                                    <img id="output5" class="upload_img"  src="~/images/cars/@Model.img5"/>
                                </li>
                            </ul>
