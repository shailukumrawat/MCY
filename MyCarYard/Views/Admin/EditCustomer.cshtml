            url: '/Admin/LoadCity',
            asyn: false,
            cache: false,
            //called on jquery ajax call success
            success: function (result) {
                $('#city').empty();
                
                    $("#city").append($("<option value='0'>Select City</option>"));
                
                //$("#city").append($("<option>Select City</option>"));
